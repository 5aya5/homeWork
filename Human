
public class Human {
        protected String name;
        protected int age;
    public String getName () {
            return name;
        }
        public void setName (String name){
            this.name = name;
        }
        public int getAge () {
            return age;
        }
        public void setAge ( int age){
            if (age < 0 || age > 150) {
                System.out.println("Не правельный возраст!");
                return;
            }else {
                this.age = age;
            }
        }

}
